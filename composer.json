{
    "name": "mobileia/mia-firebase-zf3",
    "description": "The library for firebase.",
    "version": "0.0.5",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Matias Camiletti",
            "homepage": "http://www.mobileia.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/MobileIA/mia-firebase-zf3.git"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-view": "^2.9",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "MIAFirebase\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "extra": {
        "zf": {
            "component": "MIAFirebase"
        }
    }
}
